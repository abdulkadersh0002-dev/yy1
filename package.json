{
  "name": "intelligent-auto-trading-system",
  "version": "1.0.0",
  "description": "Advanced AI-powered automated trading system with economic, news, and technical analysis",
  "type": "module",
  "main": "src/server.js",
  "scripts": {
    "prestart": "node scripts/validate-providers.js --pair=EURUSD --timeframe=M15 --bars=120 --output logs/provider-validation.json",
    "start": "node src/server.js",
    "dev": "node --watch src/server.js",
    "test": "node --test",
    "test:unit": "node --test \"tests/unit/**/*.test.js\"",
    "test:integration": "node --test \"tests/integration/**/*.test.js\"",
    "test:ci": "npm run test:unit && npm run test:integration",
    "test:coverage": "node --experimental-test-coverage --test tests/unit/**/*.test.js",
    "test:data-sources": "node scripts/test-data-sources.js",
    "db:migrate": "node scripts/run-migrations.js",
    "validate:providers": "node scripts/validate-providers.js",
    "train:adaptive-scorer": "node scripts/train-adaptive-scorer.js",
    "news:rss": "node scripts/fetch-rss-feeds.js",
    "etl:historical": "node scripts/etl/run-historical-warehouse.js",
    "etl:refresh": "node scripts/etl/run-data-refresh.js",
    "backtest:run": "node scripts/backtest/run-backtests.js",
    "deploy:stage": "node scripts/deploy/create-staging-bundle.js",
    "ci:quality-gates": "node scripts/ci/quality-gates.js",
    "ratings": "node scripts/show-ratings.js",
    "optimize": "node scripts/optimize-for-100.js",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "format:check": "prettier --check \"**/*.{js,json,md}\"",
    "format:write": "prettier --write \"**/*.{js,json,md}\"",
    "dashboard:dev": "npm --prefix clients/neon-dashboard run dev",
    "dashboard:build": "npm --prefix clients/neon-dashboard run build",
    "dashboard:preview": "npm --prefix clients/neon-dashboard run preview",
    "prepare": "npx simple-git-hooks"
  },
  "keywords": [
    "forex",
    "trading",
    "automation",
    "ai",
    "technical-analysis",
    "economic-analysis"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@vitalets/google-translate-api": "^9.2.0",
    "ajv": "^8.17.1",
    "axios": "^1.6.0",
    "bcryptjs": "^3.0.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-parse": "^5.5.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "franc": "^6.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.3",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.11",
    "otplib": "^12.0.1",
    "pg": "^8.13.1",
    "pino": "^8.15.1",
    "pino-pretty": "^10.3.0",
    "prom-client": "^14.2.0",
    "rss-parser": "^3.13.0",
    "ws": "^8.14.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "js-yaml": "^4.1.0",
    "lint-staged": "^15.2.5",
    "prettier": "^3.3.2",
    "simple-git-hooks": "^2.11.1"
  },
  "lint-staged": {
    "*.{js,mjs,cjs}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  }
}
